<?xml version='1.0' encoding='UTF-8' ?>

<guifications>
  <theme api='1'>
    <info>
      <name>Minimal Terminus</name>
      <version>1.0</version>
      <summary>Minimal Terminus Theme</summary>
      <description>Minimal Terminus Black theme</description>
      <author>Daniel Gra√±a &lt;dangra@gmail.com&gt;</author>
      <website>http://guifications.sourceforge.net</website>
    </info>
    <options>
      <date_format>%x</date_format>
      <time_format>%X</time_format>
      <warning>N/A</warning>
      <ellipsis>...</ellipsis>
    </options>


    <notification type='im-message' use_gtk='0' background='bg.png' width='512' height='60'>
      <item type='text'>
        <position value='north-west'/>
        <h_offset value='4'/>
        <v_offset value='4'/>
        <text format='%n' font='Terminus Bold 12' color='#ff0' clipping='ellipsis-end' width='0'/>
      </item>
      <item type='text'>
        <position value='north-west'/>
        <h_offset value='4'/>
        <v_offset value='24'/>
        <text format='%r' font='Terminus 9' color='#fff' clipping='ellipsis-end' width='0'/>
      </item>
    </notification>


    <notification type='chat-message' use_gtk='0' background='bg.png' width='512' height='40'>
      <item type='text'>
        <position value='north-west'/>
        <h_offset value='4'/>
        <v_offset value='4'/>
        <text format='#%c: %n' font='Terminus Bold 12' color='#0f0' clipping='ellipsis-end' width='0'/>
      </item>
      <item type='text'>
        <position value='north-west'/>
        <h_offset value='4'/>
        <v_offset value='24'/>
        <text format='%r' font='Terminus 9' color='#fff' clipping='ellipsis-end' width='0'/>
      </item>
    </notification>


    <notification type='nick-highlight' use_gtk='0' background='bg.png' width='420' height='60'>
      <item type='text'>
        <position value='north-west'/>
        <h_offset value='4'/>
        <v_offset value='4'/>
        <text format='#%c: %n' font='Terminus Bold 12' color='#f00' clipping='ellipsis-end' width='0'/>
      </item>
      <item type='text'>
        <position value='north-west'/>
        <h_offset value='4'/>
        <v_offset value='24'/>
        <text format='%r' font='Terminus 9' color='#ff0' clipping='ellipsis-end' width='0'/>
      </item>
    </notification>



    <notification type='chat-invite' use_gtk='0' background='bg.png' width='420' height='60'>
      <item type='icon'>
        <position value='north-west'/>
        <h_offset value='4'/>
        <v_offset value='4'/>
        <icon type='buddy' size='little'/>
      </item>
      <item type='text'>
        <position value='north-west'/>
        <h_offset value='42'/>
        <v_offset value='4'/>
        <text format='%n invited you to %X' font='Terminus 12' color='#fff' clipping='ellipsis-middle' width='0'/>
      </item>
    </notification>


    <notification type='warned' use_gtk='0' background='bg.png' width='420' height='60'>
      <item type='icon'>
        <position value='north-west'/>
        <h_offset value='4'/>
        <v_offset value='4'/>
        <icon type='buddy' size='little'/>
      </item>
      <item type='text'>
        <position value='north-west'/>
        <h_offset value='42'/>
        <v_offset value='4'/>
        <text format='%W warned you!' font='Terminus 12' color='#fff' clipping='ellipsis-middle' width='0'/>
      </item>
    </notification>


    <notification type='file-remote-cancel' use_gtk='0' background='bg.png' width='420' height='60'>
      <item type='icon'>
        <position value='north-west'/>
        <h_offset value='4'/>
        <v_offset value='4'/>
        <icon type='buddy' size='little'/>
      </item>
      <item type='text'>
        <position value='north-west'/>
        <h_offset value='42'/>
        <v_offset value='4'/>
        <text format='%n canceled &quot;%X&quot;' font='Terminus 12' color='#fff' clipping='ellipsis-middle' width='0'/>
      </item>
    </notification>
    <notification type='file-recv-complete' use_gtk='0' background='bg.png' width='420' height='60'>
      <item type='icon'>
        <position value='north-west'/>
        <h_offset value='4'/>
        <v_offset value='4'/>
        <icon type='buddy' size='little'/>
      </item>
      <item type='text'>
        <position value='north-west'/>
        <h_offset value='42'/>
        <v_offset value='4'/>
        <text format='%n sent &quot;%X&quot;' font='Terminus 12' color='#fff' clipping='ellipsis-middle' width='0'/>
      </item>
    </notification>
    <notification type='file-send-complete' use_gtk='0' background='bg.png' width='420' height='60'>
      <item type='icon'>
        <position value='north-west'/>
        <h_offset value='4'/>
        <v_offset value='4'/>
        <icon type='buddy' size='little'/>
      </item>
      <item type='text'>
        <position value='north-west'/>
        <h_offset value='42'/>
        <v_offset value='4'/>
        <text format='&quot;%X&quot; completed.' font='Terminus 12' color='#fff' clipping='ellipsis-middle' width='0'/>
      </item>
    </notification>

  </theme>
</guifications>
