#!/bin/bash
declare -A countryportmap
countryportmap[eu]=6666
countryportmap[uk]=6666
countryportmap[us]=6667
countryportmap[dev]=6667
port=${countryportmap[${1:-us}]:-6667}
url=${2:-http://hadoop:50030}
google-chrome --user-data-dir=/home/dan/.config/mydeco/chrome-proxy-$port \
	--proxy-server="socks5://localhost:$port" $url

